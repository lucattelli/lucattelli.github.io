<mvc:View
	controllerName="it.cyrius.internalrules.controller.Form"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:u="sap.ui.unified">
	
	<u:Shell 
		id="myShell"
		icon="{/logo}">
		<u:headItems>
			<u:ShellHeadItem
				tooltip="Home"
				icon="sap-icon://home"
				visible="true"
				press="handlePressHome" />
		</u:headItems>
	</u:Shell>

	<Page 
		title="{i18n>appTitle}"
		showNavButton="true"
		navButtonPress="onNavPress">
		<content>
			<core:HTML content='&lt;iframe src=&quot;mimes/cyrius_br_ni.pdf&quot; frameborder=&quot;0&quot; width=&quot;100%&quot; height=&quot;100%&quot;&gt;&lt;/iframe&gt;'>
			</core:HTML>
		</content>
	</Page>
</mvc:View>
